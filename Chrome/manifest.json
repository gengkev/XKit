{
   "content_scripts": [ {
      "all_frames": true,
      "exclude_matches": [ "http://www.tumblr.com/upload/image*" ],
      "css": ["xkit.css"],
      "js": [ "bridge.js", "jquery.js", "tiptip.js", "moment.js", "nano.js", "xkit.js" ],
      "matches": [ "http://www.tumblr.com/*" ]
   } ],
   "description": "The extension framework for Tumblr.",
   "homepage_url": "http://www.xkit.info/seven/",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "minimum_chrome_version": "26.0",
   "name": "XKit",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "storage", "unlimitedStorage", "management" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "web_accessible_resources": ["manifest.json", "editor.js"],
   "manifest_version": 2,
   "version": "7.0.3"
}
